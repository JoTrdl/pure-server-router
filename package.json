{
  "name": "pure-server-router",
  "description": "A sample website demonstrating the pure-server-router pattern",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Johann Troendle",
  "maintainers": [
    "Johann Troendle"
  ],
  "keywords": [
    "node",
    "react",
    "redux",
    "router",
    "universal",
    "isomorphic",
    "server-router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JoTrdl/pure-server-router.git"
  },
  "scripts": {
    "start": "NODE_ENV=production node ./dist",
    "dev": "npm-run-all -p --print-label dev:*",
    "dev:client": "NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js",
    "dev:server": "NODE_ENV=development BABEL_ENV=node babel-node ./src",
    "test": "npm-run-all test:*",
    "test:client": "jest --config config/jest.client.config.js",
    "test:server": "jest --config config/jest.server.config.js",
    "lint": "eslint ./src/** ./config/**",
    "build": "npm-run-all build:*",
    "build:clean": "rm -r -f ./dist",
    "build:client": "NODE_ENV=production webpack --config ./config/webpack.config.js",
    "build:server": "NODE_ENV=production BABEL_ENV=node babel ./src -d dist",
    "docco": "docco-plus -c ./public/docco.css -h Default -o ./public/annotated 'config/*.js' 'src/**/*.js' 'src/**/*.jsx'",
    "heroku-prebuild": "yarn install --production=false && npm run build"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "convict": "^4.0.0",
    "highlight.js": "^9.12.0",
    "history": "^4.7.2",
    "koa": "^2.3.0",
    "koa-helmet": "^3.2.0",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.2.1",
    "koa-static-cache": "^5.1.1",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "milligram": "^1.3.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-duck": "^1.0.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.82.0",
    "xxhashjs": "^0.2.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "docco-plus": "^2.0.0",
    "eslint": "^4.7.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "jest": "^21.2.0",
    "json-loader": "^0.5.7",
    "node-hot": "^1.1.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "react-test-renderer": "^16.0.0",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-module-hot-accept": "^1.0.5",
    "webpack-stats-plugin": "^0.1.5"
  },
  "engines": {
    "node": ">= 8.0"
  }
}
