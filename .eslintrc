{
  "extends": ["standard", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "react",
    "jest",
    "json"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "react/prop-types": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
